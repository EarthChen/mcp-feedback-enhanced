{
  "mcpServers": {
    "mcp-feedback-pro": {
      "command": "uvx",
      "args": ["mcp-feedback-pro@latest"],
      "timeout": 3600,
      "env": {
        "MCP_DESKTOP_MODE": "true",
        "MCP_WEB_HOST": "127.0.0.1",
        "MCP_WEB_PORT": "8765",
        "MCP_DEBUG": "false",
        "MCP_FEEDBACK_TIMEOUT": "3600"
      },
      "autoApprove": ["interactive_feedback"]
    }
  }
}
